			<Insulation_Cold>12</Insulation_Cold>
 			<Insulation_Heat>6</Insulation_Heat>
            <EquipDelay>1</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<!-- poor design prevents this simple solution
			<Medicine>5</Medicine>

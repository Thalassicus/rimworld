<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="ApparelMakeableBase">
		<defName>VAE_Apparel_HeavyCoat</defName>
		<label>heavy coat</label>
		<description>A heavy coat with insulated layers for keeping warm.</description>
		<graphicData>
			<texPath>Things/Apparel/PeltCoat/PeltCoat</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<techLevel>Neolithic</techLevel>
		<costStuffCount>80</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<statBases>
			<MaxHitPoints>180</MaxHitPoints>
			<WorkToMake>8000</WorkToMake>
			<Mass>3.8</Mass>
      <StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>2.00</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.00</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>2.5</EquipDelay>
		</statBases>
		<thingCategories Inherit="False">
			<li>Apparel</li>
		</thingCategories>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
        		<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Apparel/PeltCoat/PeltCoat</wornGraphicPath>
			<layers>
				<li>Shell</li>
			</layers>
			<tags>
                <li>IndustrialBasic</li>
				<li>Neolithic</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
				<li>Soldier</li>
            </defaultOutfitTags>
		</apparel>
		<recipeMaker>
			<recipeUsers Inherit="False">
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
				<li>CraftingSpot</li>
			</recipeUsers>
		</recipeMaker>
		<colorGenerator Class="ColorGenerator_StandardApparel" />
    </ThingDef>

    <!-- +++++ Tribal Poncho +++++ -->
    <ThingDef ParentName="ApparelMakeableBase">
		<defName>VAE_Apparel_TribalPoncho</defName>

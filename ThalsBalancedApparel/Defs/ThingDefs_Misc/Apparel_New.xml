    <!-- +++++ Lab Coat +++++ -->
    <ThingDef ParentName="ApparelMakeableBase">
		<defName>VAE_Apparel_SterileCoat</defName>
		<label>Sterile Coat</label>
		<description>A light coat worn to protect clothing from substances used in kitchens, medical wards, or laboratories.</description>
		<graphicData>
			<texPath>Things/Apparel/SterileCoat/SterileCoat</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<techLevel>Industrial</techLevel>
        <generateCommonality>0.05</generateCommonality>
		<costList>
			<Cloth>80</Cloth>
		</costList>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>7000</WorkToMake>
			<Mass>0.8</Mass>
			<ArmorRating_Sharp>0.12</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.1</ArmorRating_Blunt>
